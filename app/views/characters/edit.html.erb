<h1>Edit Character</h1>

<%= simple_form_for([@character]) do |f| %>
<%= f.input :name %>
<%= f.input :description %>
<%= f.input :price %>
<%= f.input :category, collection: [ "horreur", "humor", "animals",
  "christmas", "superheroes", "manga", "video-games", "princess", "other"] %>
<%= f.input :address %>
<%= f.input :photos, as: :file, input_html: { multiple: true } %>
<%= f.submit %>
<% end %>
