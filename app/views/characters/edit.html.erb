<h1>Edit Character</h1>

<%= simple_form_for([@character]) do |f| %>
<%= f.input :name %>
<%= f.input :description %>
<%= f.input :price %>
<%= f.input :category, collection: %w[Animation Animal Christmas Comedy Game Horror Superhero Princess Other] %>
<%= f.input :address %>
<%= f.input :photos, as: :file, input_html: { multiple: true } %>
<%= f.submit %>
<% end %>
